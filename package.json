{
  "name": "unocss-preset-dark",
  "type": "module",
  "version": "0.0.4",
  "packageManager": "pnpm@9.9.0",
  "description": "Automatic switching dark CSS var for UnoCSS.",
  "author": "TianQian-A",
  "license": "MIT",
  "funding": "https://github.com/sponsors/TianQian-A",
  "homepage": "https://github.com/TianQian-A/unocss-preset-dark.git#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TianQian-A/unocss-preset-dark.git"
  },
  "bugs": "https://github.com/TianQian-A/unocss-preset-dark.git/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@unocss/core": ">= 0.63.4 <1"
  },
  "dependencies": {
    "@unocss/rule-utils": "^0.63.4",
    "color2k": "^2.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.3.2",
    "@antfu/ni": "^0.23.0",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^22.5.4",
    "@unocss/preset-mini": "^0.63.4",
    "@unocss/preset-uno": "^0.63.4",
    "bumpp": "^9.5.2",
    "eslint": "^9.9.1",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.9.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0",
    "vite": "^5.4.3",
    "vitest": "^2.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
